  "flex-layout.row#home__section-one--slider-manual": {
    "title": "slider categories manual",
    "children": ["slider-layout#home__section-one--slider-manual"]
  },
  "slider-layout#home__section-one--slider-manual": {
    "title": "Container config and sliders manual",
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "never",
      "blockClass": "home__section-one--slider-manual",
      "infinite": false,
      "usePagination": true,
      "navigationStep": 1,
      "centerMode": "to-the-left",
      "centerModeSlidesGap": 1
    },
    "children": [
      "image#home__section-one--category-one",
      "image#home__section-one--category-two",
      "image#home__section-one--category-three",
      "image#home__section-one--category-four",
