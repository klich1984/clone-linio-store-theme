  // Slider config
  "slider-layout#home__section-one--slider-auto": {
    "title": "Slider Config",
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "autoplay": { "timeout": 7000, "stopOnHover": true },
      "showNavigationArrows": "desktopOnly",
      "blockClass": "home__section-one--slider-auto"
    }
  },
