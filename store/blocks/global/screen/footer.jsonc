{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": {
    "children": ["rich-text#footer__text--desktop"]
  },
  "rich-text#footer__text--desktop": {
    "props": {
      "blockClass": ["footer__text--desktop"],
      "font": "t-body",
      "text": "Desktop",
      "textAlignment": "CENTER",
      "textColor": "c-on-base",
      "textPosition": "CENTER"
    }
  },
  "footer-layout.mobile": {
    "children": ["flex-layout.row#footer__group"]
  },
  "flex-layout.row#footer__group": {
    "children": ["flex-layout.col#footer__group"]
  },
  "flex-layout.col#footer__group": {
    "children": ["disclosure-layout-group#footer__group"],
    "props": {
      "blockClass": "footer__group--container"
    }
  },
  "disclosure-layout-group#footer__group": {
    "title": "Men√∫s del footer",
    "children": [
      "disclosure-layout#group__about-us",
      "disclosure-layout#customer-service",
      "disclosure-layout#linio-latam",
      "disclosure-layout#newness"
    ]
  }
}
